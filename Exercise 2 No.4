#include <stdio.h>

int gcd(int u, int v) {
    while (v != 0) {
        int temp = u % v;
        u = v;
        v = temp;
    }
    return u;
}

int lcm(int u, int v) {
    return u * v / gcd(u, v);
}

int main() {
    int u, v;
    printf("Enter two positive integers: ");
    scanf("%d%d", &u, &v);
    printf("The lcm of %d and %d is %d\n", u, v, lcm(u, v));
    return 0;
}
